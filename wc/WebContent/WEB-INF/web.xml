<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>wc</display-name>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>AuthFilter</filter-name>
    <filter-class>wc.dao.AuthFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>AuthFilter</filter-name>
    <url-pattern>*.html</url-pattern>
    <url-pattern>/UserInfoServlet</url-pattern>
    <url-pattern>/MatchTobetServlet</url-pattern>
    <url-pattern>/SubmitBetServlet</url-pattern>
    <url-pattern>/SetResultServlet</url-pattern>
    <url-pattern>/MatchUnfinishedServlet</url-pattern>
    <url-pattern>/PlayOffsServlet</url-pattern>
    <url-pattern>/SetPlayoffsServlet</url-pattern>
    <url-pattern>/MatchFinishedServlet</url-pattern>
    <url-pattern>/BetedMatchServlet</url-pattern>
    <url-pattern>/ScoreBoardServlet</url-pattern>
    <url-pattern>/BetableMatchcountServlet</url-pattern>
    <url-pattern>/BetedMatchcountServlet</url-pattern>
  </filter-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
</web-app>